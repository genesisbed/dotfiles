{
  "name": "acheron",
  "version": "0.1",
  "description": "",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "root.css"
      ]
    },
    {
      "matches": [
        "*://discord.com/*"
      ],
      "css": [
        "discord.css"
      ]
    }
  ]
}